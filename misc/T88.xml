  <Entry name="SR" value="Shipper Assigned Roll Number"/>
  <Entry name="SS" value="Shipper Assigned Skid Number"/>
  <Entry name="UC" value="U.P.C. Shipping Container Code"/>
  <Entry name="UP" value="U.P.C. Consumer Package Code (1-5-5-1)"/>
