<Segment name="H5" comment="To identify the car service directive or other car movement instructions applicable to the empty movement of the car">
  <Field name="CarServiceOrderCode" min="3" max="5" validation="T240" comment="Code specifying car service order"/>
  <Field name="CityName" required="y" min="2" max="30" comment="Free-form text for city name"/>
  <Field name="StateOrProvinceCode" required="y" min="2" max="2" comment="Code (Standard State/Province) as defined by appropriate government agency"/>
</Segment>
