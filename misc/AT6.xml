<Segment name="AT6" comment="To transmit international manifest data">
  <Field name="InternationalDutiableStatusCode" min="2" max="2" validation="T1610" comment="Code identifying the dutiable status of an international shipment"/>
  <Field name="ImportExportCode" min="1" max="1" validation="T512" comment="Code identifying the type of foreign trade shipments handling capability"/>
  <Field name="TransportationTermsCode" required="y" min="3" max="3" validation="T335" comment="Code identifying the trade terms which apply to the shipment transportation responsibility"/>
</Segment>
