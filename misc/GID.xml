  <Field name="Name1" min="1" max="60" comment="Free-form name"/>
  <Field name="GenderCode" min="1" max="1" validation="T1068" comment="Code indicating the sex of the individual"/>
  <Field name="Name2" required="y" min="1" max="60" comment="Free-form name"/>
</Segment>
