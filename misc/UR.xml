  <Field name="ApprovalCode" min="1" max="1" comment="Code specifying the determination arrived at by the Utilization Review Committee or Peer Review Organization (PRO)"/>
  <Field name="Quantity" type="double" required="y" min="1" max="15" comment="Numeric value of quantity"/>
</Segment>
